                           </children>
                           <StackPane.margin>
                              <Insets top="20.0" />
                           </StackPane.margin>
                        </Group>
                     </children>
                  </StackPane>
               </children>
            </Group>
            <Separator orientation="VERTICAL" prefHeight="200.0" prefWidth="1.0" style="-fx-background-color: transparent;">
               <padding>
                  <Insets left="-5.0" right="-5.0" />
               </padding>
            </Separator>
         </children>
      </HBox>
   </children>
